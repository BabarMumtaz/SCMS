<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Feature Test Suite">

	<listeners>
		<listener class-name="com.LilyCargo.Listeners.AllureReportListener"/>
	</listeners>

	<test name="Smoke Tests">
		<groups>
			<run>
				<include name="smoke"/>
				<exclude name="regression"/>
			</run>
		</groups>
		<classes>
			<class name="com.LilyCargo.TestCases.LoginUsingConfigFileTestCase"/>
		</classes>
	</test>

</suite>